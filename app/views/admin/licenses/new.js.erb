$("#edunModal .modal-content").html("<%= j render 'admin/licenses/new', license: License.new, client: client %>");

$('#edunModal select.select2').select2({
    theme: 'bootstrap4',
});
$('#edunModal .datetimepicker').each(function (index, value) {
    const $this = $(value);
    const options = $.extend({dateFormat: 'd/m/y'}, $this.data('options'));
    $this.attr('placeholder', options.dateFormat);
    $this.flatpickr(options);
});
